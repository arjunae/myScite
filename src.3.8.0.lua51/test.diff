+function split(s,re)
+	local i1 = 1
+	local sz = #s
+	local ls = {}
+	while true do
+		local i2,i3 = s:find(re,i1)
+		if not i2 then
+			append(ls,s:sub(i1))
+			return ls
+		end
+		append(ls,s:sub(i1,i2-1))
+		i1 = i3+1
+		if i1 >= sz then return ls end
+	end
